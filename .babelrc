{
  "comments": false,
  "env": {
    "main": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "node": 7
            }
          }
        ]
      ]
    },
    "renderer": {
      "presets": [
        [
          "env",
          {
            "modules": false
          }
        ]
      ]
    },
    "web": {
      "presets": [
        [
          "env",
          {
            "modules": false
          }
        ]
      ]
    },
    "nw": {
      "presets": [
        [
          "env",
          {
            "modules": false
          }
        ]
      ]
    }
  },
  "plugins": [
    "transform-runtime",
    "transform-object-rest-spread"
  ],
  "ignore": [
    "vue.min.js",
    "axios.min.js",
    "vue.common.min.js"
  ]
}
