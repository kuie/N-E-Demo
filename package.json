{
  "name": "evt1",
  "version": "0.2.0",
  "author": "kuie881231@gmail.com",
  "description": "An electron-vue project",
  "license": "",
  "private": true,
  "scripts": {
    "installModules": "node ./config/installModules.js && yarn",
    "hashMap": "node ./update/electron.js",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:electron": "cross-env NODE_ENV=production BUILD_TARGET=electron node .electron-vue/build.js",
    "build:nw": "cross-env NODE_ENV=production BUILD_TARGET=nw node .electron-vue/build.js",
    "build:web": "cross-env NODE_ENV=production  BUILD_TARGET=web node .electron-vue/build.js",
    "dev:electron": "cross-env NODE_ENV=development node .electron-vue/dev-runner.js",
    "dev:nw": "cross-env NODE_ENV=development node .electron-vue/dev-server.js",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "asar": "^0.14.5",
    "axios": "^0.18.0",
    "dexie": "^2.0.4",
    "iview": "^3.1.2",
    "uuid": "^3.3.2",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "electron-updater": "^3.1.2"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.5",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-istanbul": "^5.0.1",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "cfonts": "^2.2.1",
    "chai": "^4.2.0",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "^3.0.2",
