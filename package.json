    "postcss-loader": "^2.1.6",
    "require-dir": "^1.0.0",
    "sass-loader": "^7.1.0",
    "spectron": "^3.7.1",
    "style-loader": "^0.23.0",
    "url-loader": "^0.6.2",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.0.5",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.17",
